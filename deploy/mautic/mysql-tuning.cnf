[mysqld]
# Memory-optimised config for small VPS (3.7 GB RAM shared with other services)
performance_schema      = OFF
innodb_buffer_pool_size = 64M
key_buffer_size         = 2M
tmp_table_size          = 8M
max_heap_table_size     = 8M
max_connections         = 30
table_open_cache        = 200
innodb_log_buffer_size  = 4M
